 @font-face {
 font-family: 'walkway_semiboldregular';
 src: url('walkway_semibold-webfont.eot');
 src: url('walkway_semibold-webfont.eot?#iefix') format('embedded-opentype'), url('walkway_semibold-webfont.woff') format('woff'), url('walkway_semibold-webfont.ttf') format('truetype'), url('walkway_semibold-webfont.svg#walkway_semiboldregular') format('svg');
 font-weight: normal;
 font-style: normal;
}
* {
	margin:0px;
	padding:0px;
}
body {
	margin:0px;
	padding:0px;
	background:url(../images/body-bg.jpg);
	font-family:"walkway_semiboldregular";
}
.fl {
	float:left;
}
.fr {
	float:right;
}
header {
	background:#2D9205;
	float: left;
	padding: 0 5% 1% 10%;
	width: 85%;
}
header .header {
	width: 91%;
	margin:0 auto;
}
.logo_scetion {
	font-family:"walkway_semiboldregular";
	font-size:145px;
	float:left;
	width:30%;
	color:#fbfcfc;
}
.logo_scetion a {
	text-decoration:none;
	color:#fbfcfc;
}
.search_section {
	float:right;
	padding-top:50px;
	width:62%;
}
.search_section_second {
	float:left;
	padding-top:50px;
	width:51%;
}
.text_area {
	float:right;
	box-shadow: 0 0 2px 2px #227804;
	border:2px solid #227804;
	border-radius:8px 8px 8px 8px;
	-moz-border-radius:8px 8px 8px 8px;
	-webkit-border-radius:8px 8px 8px 8px;
	height:60px;
	padding:2px 15px;
	font-family:"walkway_semiboldregular";
	font-size:50px;
	color:#333;
}
.search_button {
	width:13%;
	float:left;
	background:url(../images/search.png) no-repeat;
	height:68px;
	border:none;
	cursor:pointer;
}
section {
	width:100%;
	float:left;
	margin-top:10px;
}
section .container_section {
	width:100%;
	float:left;
}
.ads_section {
	float:left;
	font-size:48px;
	text-align:center;
	line-height:150px;
	width:100%;
}
.big_green_box {
	float:left;
	background:url(../images/green_bg.jpg) repeat;
	padding: 5px 2% 20px 2%;
	width: 96%;
	border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	margin-bottom:10px;
}
.bar_section {
	width:31%;
	float:left;
	padding:28px 0px 0px 5%;
}
.bar {
	width:10%;
	float:left;
	background:#33ab17;
	border-radius:8px;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	height:49px;
}
.text_section {
	width:90%;
	float:left;
	padding:0 5%;
}
.text_section h1 {
	font-size:55px;
	color:#FFF;
}
.text_section p {
	font-size:29px;
	color:#fdfeff;
	padding:5px 0px 5px 0px;
}
.responses {
	width:70%;
	float:left;
	font-size:29px;
	color:#fdfeff;
}
.stars {
	width:30%;
	float:right;
	font-size:29px;
	color:#fdfeff;
}
.small_box {
	width:80%;
	float:left;
	border-radius:8px;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	font-size:50px;
	background:#257108;
	color:#fdfeff;
	text-align:center;
	margin-top:10px;
}
.small_box:hover {
	background:#b10606;
}
.ads_section_main {
	width:10%;
	float:left;
}
.green_box_main {
	width:78%;
	float:left;
	padding: 0 10%;
}
.green_box_main_home {
	width:49%;
	float:left;
	padding-left:9%;
}
.ads_section_main_sec {
	float:right;
	width:10%;
}
.small_box_main {
	width:20%;
	float:left;
	margin-left:2%;
}
.small_box_main ul {
	margin:0px;
	padding:0px;
	list-style:none;
}
.small_box_main ul li {
	background: none repeat scroll 0 0 #257108;
	border-radius: 8px 8px 8px 8px;
	color: #FDFEFF;
	font-size: 250%;
	margin-bottom: 10px;
	text-align: center;
	width:90%;
}
.small_box_main ul li a {
	text-decoration:none;
	color:#FFF;
	display:block;
	padding:0 5%;
	border-radius: 8px 8px 8px 8px;
}
.small_box_main ul li a.active {
	background:#0c5707;
	border-radius: 8px 8px 8px 8px;
}
.small_box_main ul li a:hover {
	display:block;
	background:#b10606;
	border-radius: 8px 8px 8px 8px;
}
footer {
	width:100%;
	float:left;
	background:#2d9205;
	padding:10px 0px;
	margin:15px 0px 0px 0px;
}
footer .footer {
	margin:0 auto;
	width:78%;
}
.footer_button {
	float:left;
	background:#23a405;
	border-radius:8px;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	font-size:55px;
	padding:1px 20px;
	text-align:center;
	-moz-box-shadow: 0 0 5px #fff;
	-webkit-box-shadow: 0 0 5px#fff;
	box-shadow: 0 0 5px #fff;
	color:#fff;
	margin:10px 7%;
}
.footer_text {
	width:100%;
	float:left;
	font-size:36px;
	color:#fff;
	text-align:center;
	padding:30px 0px;
}
/* viable-poll css*/
.green_box_main_sec {
	width:78%;
	float:left;
}
.google_search_box {
	width:97%;
	float:left;
	border:3px solid #020304;
	border-radius:8px;
	padding:1%;
	font-size:80px;
	font-weight:bold;
	text-align:center;
	margin-bottom:20px;
}
.yes_bar_sec {
	width:100%;
	float:left;
}
.yes_bar {
