	background:#216e03;
	border-radius:8px;
	padding:5px;
	border:none;
	color:#FFF;
	font-family:"walkway_semiboldregular";
	margin-bottom:10px;
}
.yes_bar2 {
	width:55%;
	float:left;
	font-size:40px;
	background:#216e03;
	border-radius:8px;
	padding:5px;
	border:none;
	color:#FFF;
	font-family:"walkway_semiboldregular";
	margin-bottom:10px;
}
.yes_bar3 {
	width:30%;
	float:left;
	font-size:40px;
	background:#216e03;
	border-radius:8px;
	padding:5px;
	border:none;
	color:#FFF;
	font-family:"walkway_semiboldregular";
	margin-bottom:10px;
}
.yes_bar4 {
	width:60%;
	float:left;
	font-size:40px;
	background:#216e03;
	border-radius:8px;
	padding:5px;
	border:none;
	color:#FFF;
	font-family:"walkway_semiboldregular";
	margin-bottom:10px;
}
.one_button {
	width:auto;
	padding:10px;
	border-radius:8px;
	float:left;
	margin-right:5%;
	background:#195004;
	color:#FFF;
	font-size:40px;
	margin-top:5px;
}
.poll_row {
	width:100%;
	float:left;
}
.poll_row h2 {
	font-size:60px;
	float:left;
	width:100%;
	text-align:center;
	color:#FFF;
}
.poll_textarea {
	width:98%;
	float:left;
	background-image:url(../images/body-bg.jpg);
	border-radius:8px;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	margin:5px 0px;
	padding:1%;
	border:none;
}
.poll_text {
	width:98%;
	float:left;
	padding:1%;
	border:none;
	background-image:url(../images/body-bg.jpg);
	border-radius:8px;
	-moz-border-radius:8px;
	-webkit-border-radius:8px;
	margin:5px 0px;
	font-size:38px;
	color:#000;
	text-align:center;
}
.about_text {
	width:98%;
	float:left;
	padding:1%;
}
.about_text p {
	font-size:30px;
	color:#FFF;
	text-align:center;
	padding:15px;
}
.white_line {
	width:100%;
	float:left; /*background:#fff;*/
	height:15px;
}
.poll_section {
	width:96%;
	padding:2%;
}
.poll_section h1 {
	font-size:100px;
	float:left;
	width:100%;
	text-align:center;
	color:#FFF;
	border-bottom:2px solid #feffff;
	padding-bottom:20px;
}
.clr{clear:both; float:left; width:100%; height:10px;}