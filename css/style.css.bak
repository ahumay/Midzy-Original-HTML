	float:left;
	font-size:40px;
	background:#216e03;
	border-radius:8px;
	padding:5px;
	border:none;
	color:#FFF;
	font-family:"walkway_semiboldregular";
	margin-bottom:10px;
}
.yes_bar1 {
	width:52%;
	float:left;
